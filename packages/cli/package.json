{
  "name": "@mosmos_21/vault-key-cli",
  "version": "0.2.0",
  "description": "VaultKey の CLI ツール。コマンドラインから機密情報を管理できます。",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "bin": {
    "vaultkey": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "keywords": [
    "security",
    "secrets",
    "vault",
    "cli",
    "password-manager",
    "webauthn",
    "passkey",
    "typescript"
  ],
  "author": "Takahito Hashimoto",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mosmos21/vault-key"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@mosmos_21/vault-key-core": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "hono": "^4.10.7",
    "open": "^11.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/open": "^6.2.1",
    "@types/prompts": "^2.4.9",
    "execa": "^9.6.0",
    "typescript": "^5.9.3"
  }
}
