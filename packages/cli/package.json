{
  "name": "@mosmos_21/vault-key-cli",
  "version": "0.0.0",
  "description": "VaultKey の CLI ツール。コマンドラインから機密情報を管理できます。",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vaultkey": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "security",
    "secrets",
    "vault",
    "cli",
    "password-manager",
    "webauthn",
    "passkey",
    "typescript"
  ],
  "author": "Takahito Hashimoto",
  "license": "MIT",
  "dependencies": {
    "@mosmos_21/vault-key-core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
