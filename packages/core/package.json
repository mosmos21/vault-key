{
  "name": "@mosmos_21/vault-key-core",
  "version": "0.0.0",
  "description": "VaultKey のコアライブラリ。機密情報の暗号化保存、WebAuthn Passkey 認証、監査ログなどの機能を提供します。",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && cpy 'src/database/migrations/**/*.sql' dist/database/migrations"
  },
  "keywords": [
    "security",
    "secrets",
    "vault",
    "encryption",
    "webauthn",
    "passkey",
    "credentials",
    "typescript"
  ],
  "author": "Takahito Hashimoto",
  "license": "MIT",
  "devDependencies": {
    "cpy-cli": "^6.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
